{
  "private": true,
  "scripts": {
    "build": "cp .env.stable .env && yarn run context:manifest && node enrichments.mjs && node routes.mjs && nuxt build",
    "dev": "cp .env.stable .env && yarn run context:manifest && node enrichments.mjs && nuxt dev",
    "build:canary": "cp .env.canary .env && yarn run context:manifest && node enrichments.mjs && node routes.mjs && nuxt build",
    "dev:canary": "cp .env.canary .env && yarn run context:manifest && node enrichments.mjs && nuxt dev",
    "context:manifest": "uniform context manifest download --output ./lib/context/context-manifest.json",
    "pull": "run-s pull:components pull:compositions",
    "pull:compositions": "uniform presentation composition pull ./data/canvas/compositions",
    "pull:components": "uniform presentation component pull ./data/canvas/components",
    "push": "run-s push:components push:compositions",
    "push:compositions": "uniform presentation composition push ./data/canvas/compositions",
    "push:components": "uniform presentation component push ./data/canvas/components",
    "pull:context:signals": "uniform context signal pull ./data/turbo.yaml",
    "pull:context:enrichments": "uniform context enrichment pull ./data/turbo.yaml",
    "pull:context:aggregates": "uniform context aggregate pull ./data/turbo.yaml",
    "pull:context:quirks": "uniform context quirk pull ./data/turbo.yaml",
    "pull:context:tests": "uniform context test pull ./data/turbo.yaml",
    "pull:context": "run-s pull:context:signals pull:context:enrichments pull:context:quirks pull:context:aggregates",
    "push:context:signals": "uniform context signal push ./data/turbo.yaml",
    "push:context:enrichments": "uniform context enrichment push ./data/turbo.yaml",
    "push:context:aggregates": "uniform context aggregate push ./data/turbo.yaml",
    "push:context:quirks": "uniform context quirk push ./data/turbo.yaml",
    "push:context:tests": "uniform context test push ./data/turbo.yaml",
    "push:context": "run-s push:context:signals push:context:enrichments push:context:quirks push:context:aggregates",
    "pull:enrichments": "node enrichments.mjs",
    "copy:edge-netlify": "cp node_modules/@uniformdev/context-edge-netlify/dist/index.deno.js lib/context",
    "postinstall": "yarn copy:edge-netlify"
  },
  "dependencies": {
    "@contentful/rich-text-html-renderer": "^15.13.1",
    "@funken-studio/sitemap-nuxt-3": "^3.2.0",
    "@kevinmarrec/nuxt-pwa": "^0.8.0",
    "@nuxt/content": "^2.1.1",
    "@nuxtjs/algolia": "^1.1.4",
    "@nuxtjs/robots": "^2.5.0",
    "@nuxtjs/tailwindcss": "^5.3.5",
    "@prismicio/client": "^6.7.1",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@uniformdev/canvas": "17.3.0",
    "@uniformdev/canvas-algolia": "17.3.0",
    "@uniformdev/canvas-cloudinary": "17.1.0",
    "@uniformdev/canvas-contentful": "17.3.0",
    "@uniformdev/canvas-prismic": "^17.3.0",
    "@uniformdev/canvas-vue": "17.3.0",
    "@uniformdev/cli": "17.3.0",
    "@uniformdev/context": "17.3.0",
    "@uniformdev/context-edge-netlify": "^17.3.0",
    "@uniformdev/context-vue": "^17.3.0",
    "@uniformdev/uniform-nuxt": "17.3.0",
    "algoliasearch": "^4.14.2",
    "apexcharts": "^3.36.0",
    "contentful": "^9.2.5",
    "dotenv": "^16.0.3",
    "draft-convert": "^2.1.13",
    "duration-fns": "^3.0.1",
    "isomorphic-unfetch": "^3.1.0",
    "node-fetch-native": "^0.1.8",
    "npm-run-all": "^4.1.5",
    "nuxt": "3.0.0-rc.11",
    "prismic-dom": "^2.2.7",
    "rss": "^1.2.2",
    "universal-cookie": "^4.0.4",
    "vue-plausible": "^1.3.2",
    "vue3-apexcharts": "^1.4.1"
  }
}
